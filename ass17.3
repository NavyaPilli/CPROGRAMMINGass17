//TO PERFORM ADDITION MULTIPLICATION, DIVISION, SUBTRACTION BY USING FUNCTIONS
#include<stdio.h>
int add(int a,int b);
int sub(int a,int b);
int mul(int a,int b);
int div(int a,int b);
int mod(int a,int b);
int main()
{
	int x,y;
	scanf("%d %d",&x,&y);
	int c=add(x,y);
	int d=sub(x,y);
	int e=mul(x,y);
	int f=div(x,y);
	int g=mod(x,y);
}
int add(int a,int b)
{
	printf("%d\n",a+b);
}
int sub(int a,int b)
{
	printf("%d\n",a-b);
}
int mul(int a, int b)
{
	printf("%d\n",a*b);
}
int div(int a,int b)
{
	printf("%d\n",a/b);
}
int mod(int a,int b)
{
	printf("%d\n",a%b);
}
